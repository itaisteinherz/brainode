{
  "name": "brainode",
  "version": "1.1.0",
  "description": "A Node.js interpreter for Brainfuck",
  "main": "src/index.js",
  "repository": "https://github.com/itaisteinherz/brainode",
  "author": "Itai Steinherz <itaisteinherz@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "xo && ava"
  },
  "dependencies": {
    "readline-sync": "^1.4.6"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "sinon": "^1.17.7",
    "xo": "^0.17.1"
  },
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "brainfuck",
    "interpreter"
  ],
  "xo": {
    "rules": {
      "quotes": ["error", "double"],
      "arrow-parens": ["error", "always"]
    }
  }
}
